    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "outDir": "./dist",
    "paths": {
      "@/*": ["../shared/*"]
    },
    "skipLibCheck": true,
    "strict": true,
    "target": "es2016"
  },
  "include": ["**/*.ts", "../shared/**/*"]
}
